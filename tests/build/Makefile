ifndef config
  config=linux
endif
export config

PROJECTS := dummy-user

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

dummy-user: 
	@echo "==== Building dummy-user ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f dummy-user.make

clean:
	@${MAKE} --no-print-directory -C . -f dummy-user.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   linux"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   dummy-user"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
