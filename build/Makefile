# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=linux
endif
export config

PROJECTS := c-hat

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

c-hat: 
	@echo "==== Building c-hat ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f c-hat.make

clean:
	@${MAKE} --no-print-directory -C . -f c-hat.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   linux"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   c-hat"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
